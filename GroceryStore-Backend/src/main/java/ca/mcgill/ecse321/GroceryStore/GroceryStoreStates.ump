class PickupOrder{
  depend ca.mcgill.ecse321.GroceryStore.model.PickupOrder;
  PickupStatus{
    InCart{
      order() -> Ordered;
    }
    Ordered{
      prepare() -> Prepared;
    }
    Prepared{
     pay() -> PickedUp;
    }
    PickedUp{
    }
  }
}


class DeliveryOrder{
  depend ca.mcgill.ecse321.GroceryStore.model.DeliveryOrder;
  ShippingStatus{
    InCart{
      order() -> Ordered;
    }
    Ordered{
      pay() -> Prepared;
    }
    Prepared{
     deliver() -> Delivered;
    }
    Delivered{
    }
  }
}



class Employee{
  depend ca.mcgill.ecse321.GroceryStore.model.Employee;
  workingStatus{
    Hired{
      fireEmployee() -> Fired;
    }
    Fired{}
  }
}